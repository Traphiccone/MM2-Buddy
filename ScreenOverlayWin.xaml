<Window x:Class="MM2Buddy.ScreenOverlayWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MM2Buddy"
        mc:Ignorable="d"
        Title="ScreenOverlayWin" Height="450" Width="800">
    <Window.Background>
        <SolidColorBrush Color="Transparent" Opacity="0"/>
    </Window.Background>
    <Grid>
        <Image x:Name="streamOverlayOut" Stretch="None"/>
    </Grid>
</Window>
